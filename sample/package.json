{
  "name": "sc-update-module",
  "version": "1.0.0",
  "description": "Sample module that hooks into HOOPS Communicator and allows for client-side updates to stream cache models",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd sample/server && node app-server.js",
    "client": "cd sample/client && npm run start"
  },
